{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "object-shorthand": [2, "consistent"],
    "no-use-before-define": 0,
    "no-console": 0,
    "arrow-body-style": 0,
    "arrow-parens": [2, "as-needed"],
    "react/sort-comp": [0, { "order": ["static-methods", "lifecycle", "everything-else", "render"] }]
  }
}